{
  "name": "fe-auto-config-cli",
  "version": "0.0.1",
  "description": "add lint config and install dependencies auto",
  "type": "module",
  "bin": {
    "fe-auto-config-cli": "./dist/fe-auto-config-cli.cjs"
  },
  "files": [
    "template",
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/build.js",
    "lint": "eslint src/* --fix",
    "lint:ci": "eslint src/*",
    "format": "prettier --write .",
    "format:ci": "prettier --check  .",
    "changeset": "changeset",
    "version": "changeset version && pnpm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RanTing1992/fe-auto-config-cli.git"
  },
  "keywords": [
    "eslint",
    "git-hooks",
    "prittier",
    "Stylelint",
    "commitlint",
    "husky",
    "lint-staged"
  ],
  "author": "RanTing",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RanTing1992/fe-auto-config-cli/issues"
  },
  "homepage": "https://github.com/RanTing1992/fe-auto-config-cli#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.22",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@astrojs/cli-kit": "^0.4.1",
    "arg": "^5.0.2",
    "execa": "^8.0.1"
  }
}
